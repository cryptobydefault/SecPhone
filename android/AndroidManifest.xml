<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.secphone"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
    
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.secphone.LandingActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.secphone.GenerateKeysActivity"
            android:label="Generate Keys" >
        </activity>
        
        <activity
            android:name="com.secphone.ValidateEmailActivity"
            android:label="Validate Email" >
        </activity>
        
        <activity
            android:name="com.secphone.HomeActivity"
            android:label="Home" >
        </activity>
        
         <activity
            android:name="com.secphone.NewMessageActivity"
            android:label="New Message" >
        </activity>
           
       	<activity
            android:name="com.secphone.ReadMessageActivity"
            android:label="Message" >
        </activity>
         
    </application>

</manifest>
